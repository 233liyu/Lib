.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 200rpx;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 180rpx;
  height: 180rpx;
  margin: 40rpx;
  border-radius: 50%;
  border-width: 3rpx;
  border-color: #ffffff;
}

.userinfo-nickname {
    color: #ffffff;
}

.OrederInfo{
 background: #ffffff;
 flex: row;
 padding: 30rpx 10rpx 30rpx 10rpx;
}
.failed
{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100rpx;
  margin-left: 150rpx;
}
.success
{ display: flex;
  flex-direction: column;
  align-items: center;
  width: 100rpx;
  margin-left: 500rpx;
  margin-top: -130rpx;
}

.navigator-box-show {
  opacity: 1;
  width: 100%;
  height: 100rpx;
  margin: auto;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 34rpx;
  transform: translateY(0);
  
}
.navigator:after {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 65rpx;
  right: 30rpx;
  height: 1px;
  border-bottom: 1rpx solid #D8D8D8;
  color: #D8D8D8;
}
.navigator-text {
  flex: 1;
  width: 200rpx;
  /*border:1px solid #888888;*/
  font-size: 30rpx;
  padding-top:15rpx; 
  margin-left: 60rpx;
}
.navigator-arrow {
  padding-right: 26rpx;
  display: inline-block;
  height: 35rpx;
  width: 10rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  right: 28rpx;
  margin-top: -35rpx;
}
